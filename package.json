{
  "name": "react-hooks-compose",
  "version": "1.0.0",
  "description": "Compose React Hooks",
  "main": "dist/main.js",
  "scripts": {
    "test": "jest --coverage --testPathPattern=__tests__/index.test.js",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "watch": "webpack --watch",
    "build": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helloitsjoe/react-hooks-compose.git"
  },
  "keywords": [
    "react",
    "hooks",
    "compose",
    "composition",
    "testing"
  ],
  "author": "Joe Boyle",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/helloitsjoe/react-hooks-compose/issues"
  },
  "files": [
    "/dist",
    "README.md"
  ],
  "husky": {
    "hooks": {
      "pre-push": "npm t -- --silent"
    }
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./src/setupTests.js"
    ]
  },
  "homepage": "https://github.com/helloitsjoe/react-hooks-compose#readme",
  "peerDependencies": {
    "react": "^16.8.6"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-react-simple": "^1.0.2",
    "coveralls": "^3.0.5",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.1.0",
    "eslint-config-helloitsjoe": "^1.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.2",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "react-dom": "^16.8.6",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-simple": "^1.4.0"
  },
  "dependencies": {
    "react": "^16.8.6"
  }
}
